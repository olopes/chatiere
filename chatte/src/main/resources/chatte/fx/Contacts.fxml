<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="contactsPanel" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chatte.fx.ContactsController">
   <children>
      <VBox spacing="5.0">
         <children>
            <Label alignment="CENTER_RIGHT" text="Nick:" />
            <TextField fx:id="nick" />
            <Label alignment="CENTER_RIGHT" text="Host:" />
            <TextField fx:id="host" promptText="192.168.1.123" />
            <Label alignment="CENTER_RIGHT" text="Port:" />
            <TextField fx:id="port" text="6666" />
         </children>
      </VBox>
      <HBox alignment="TOP_CENTER" spacing="10.0">
         <children>
          <Button fx:id="okButton" mnemonicParsing="false" onAction="#doConnect" text="Connect" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
